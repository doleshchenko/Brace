@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
@section Styles
{}

<div id="shell">
    <ul id="output" class="output-list" data-bind="foreach: commandResults">
        <li>
            <div data-bind="template: {name: displayMode}, css: statusStyle">
            </div>
        </li>
    </ul>
    <div class="command-line">
        <label>\></label>
        <div>
            <input type="text" id="commandLineValue" data-bind="value: commandLineValue, hasFocus: commandLineHasFocus, enable: commandLineEnabled, event: {keypress: onKeyPress, change: onCommandChange}" />
        </div>
    </div>
</div>
<script type="text/html" id="plain">
    <div data-bind="text: result.item"></div>
</script>
<script type="text/html" id="table">
    <table>
        <thead>
            <tr data-bind="foreach: result.columnNames">
                <th>
                    <span data-bind="text: $data"></span>
                </th>
            </tr>
        </thead>
        <tbody data-bind="foreach: result.items">
            <tr data-bind="foreach: $parent.result.columnNames">
                <td data-bind="text: $parent[$data]"></td>
            </tr>
        </tbody>
    </table>
</script>